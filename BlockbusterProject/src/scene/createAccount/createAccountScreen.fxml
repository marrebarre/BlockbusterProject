      <TextField fx:id="lastNametxtField" layoutX="277.0" layoutY="60.0" prefHeight="25.0" prefWidth="140.0" promptText="Last Name" />
      <TextField fx:id="emailtxtField" layoutX="122.0" layoutY="96.0" prefHeight="25.0" prefWidth="297.0" promptText="E-mail Address" />
      <TextField fx:id="passwordtxtField" layoutX="122.0" layoutY="134.0" prefHeight="25.0" prefWidth="297.0" promptText="Password" />
      <TextField fx:id="addresstxtField" layoutX="122.0" layoutY="171.0" prefHeight="25.0" prefWidth="297.0" promptText="Address" />
      <TextField fx:id="phonetxtField" layoutX="122.0" layoutY="206.0" prefHeight="25.0" prefWidth="297.0" promptText="Phone Number" />
      <Label fx:id="successLabel" layoutX="123.0" layoutY="282.0" prefHeight="21.0" prefWidth="96.0" textFill="#00ff0d">
         <font>
            <Font name="SansSerif Bold" size="15.0" />
         </font>
      </Label>
      <Label fx:id="firstNameError" layoutX="105.0" layoutY="60.0" prefHeight="17.0" prefWidth="11.0" text="*" textFill="RED" visible="false">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Label fx:id="emailError" layoutX="105.0" layoutY="96.0" prefHeight="17.0" prefWidth="11.0" text="*" textFill="RED" visible="false">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Label fx:id="passwordError" layoutX="105.0" layoutY="134.0" prefHeight="17.0" prefWidth="11.0" text="*" textFill="RED" visible="false">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Label fx:id="addressError" layoutX="105.0" layoutY="171.0" prefHeight="17.0" prefWidth="11.0" text="*" textFill="RED" visible="false">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Label fx:id="phoneError" layoutX="105.0" layoutY="206.0" prefHeight="17.0" prefWidth="11.0" text="*" textFill="RED" visible="false">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Label fx:id="lastNameError" layoutX="265.0" layoutY="60.0" prefHeight="17.0" prefWidth="11.0" text="*" textFill="RED" visible="false">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Label fx:id="unfilledError" layoutX="116.0" layoutY="241.0" prefHeight="37.0" prefWidth="258.0" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="122.0" text="Create Account" textFill="WHITE">
         <font>
            <Font size="32.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
