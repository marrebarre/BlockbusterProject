            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="backToLoginbtn" layoutX="298.0" layoutY="309.0" mnemonicParsing="false" onAction="#backToLogin" prefHeight="49.0" prefWidth="174.0" text="Back to login &gt;&gt;" />
